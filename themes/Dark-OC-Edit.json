{
    "$schema": "vscode://schemas/color-theme",
    "name": "Dark OC Edit (Visual Studio)",
    "include": "./dark_vs.json",
    "tokenColors":[
		{
			"scope": "constant.language",
			"settings": {
				"foreground": "#4887bb"
			}
		},
		{
            "name": "Keyword",
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#4887bb"
			}
		},
        {
            "name": "Function Name",
            "scope": "entity.name.function",
            "settings": {
                "foreground": "#DCDCAA"
            }
        },
        {
            "name": "Library function",
            "scope": "support.function",
            "settings": {
                "foreground": "#4EC9B0"
            }
        },
        {
            "name": "Library function",
            "scope": "support.function.any-method",
            "settings": {
                "foreground": "#DCDCAA"
            }
        },
        {
            "name": "Library function",
            "scope": "variable.other",
            "settings": {
                "foreground": "#9cdcfe"
            }
        },
        {
            "name": "Module",
            "scope": "constant.other.module",
            "settings": {
                "foreground": "#4EC9B0"
            }
        },
        {
            "name": "OpenComputers Function",
            "scope": "OCFunction",
            "settings": {
                "foreground": "#4EC9B0"
            }
        },
        {
            "name": "OpenComputers Color Red",
            "scope": "OCColorRed",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#FF0000"
            }
        },
        {
            "name": "OpenComputers Color White",
            "scope": "OCColorWhite",
            "settings": {
